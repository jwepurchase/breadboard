<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0"
    jcr:description="Template with focus on content."
    jcr:primaryType="cq:Template"
    jcr:title="breadboard Home Page"
    allowedPaths="[/content/breadboard/.+]"
    ranking="{Long}100">
    <jcr:content
        jcr:primaryType="cq:PageContent"
        sling:resourceType="breadboard/components/structure/pageHome">
        <logo
            jcr:primaryType="nt:unstructured"
            sling:resourceType="breadboard/components/structure/logo"/>
        <title
            jcr:primaryType="nt:unstructured"
            sling:resourceType="breadboard/components/content/title"/>
        <topnav
            jcr:primaryType="nt:unstructured"
            sling:resourceType="breadboard/components/structure/topnav"/>
        <hero
            jcr:primaryType="nt:unstructured"
            sling:resourceType="breadboard/components/content/slickSlider"/>
        <rows
            jcr:primaryType="nt:unstructured"
            sling:resourceType="wcm/foundation/components/parsys">
            <content
                jcr:primaryType="nt:unstructured"
                sling:resourceType="breadboard/components/structure/rowComponent">
                <col-1
                    jcr:primaryType="nt:unstructured"
                    mediaSize="small"
                    width="10"
                    offset="1">
                    <par
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="wcm/foundation/components/parsys"/>
                </col-1>
            </content>
        </rows>
    </jcr:content>
</jcr:root>
